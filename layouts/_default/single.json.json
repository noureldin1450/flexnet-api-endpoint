{
    "title": {{ .Params.title | jsonify }},
    "trailer": {{ .Params.trailer | jsonify }},
    "year": {{ .Params.year | jsonify }},
    "date": {{ .Params.date | jsonify }},
    "slug": {{ .Params.slug | jsonify }},
    "duration": {{ .Params.duration | jsonify }},
    "imdbid": {{ .Params.imdbid | jsonify }},
    {{ if .Params.moviebg }}
    "moviebg": {{ .Params.moviebg | jsonify }},
    {{end}}
    "moviecover": {{ .Params.moviecover | jsonify }},
    "rating": {{ .Params.rating | jsonify }},
    "stars":[
        {{- range  $index, $e := .Params.stars -}}
        {{- if $index -}}, {{- end -}}
         {{ . | jsonify }}
        {{- end -}}
    ],
    "tags":[
        {{- range $index, $e := .Params.tags -}}
        {{- if $index -}}, {{- end -}}
        {{ $tag:= replace . " " "" }}
        {{ $tag | jsonify }}
        {{- end -}}
    ],
    {{ if .Params.fushaarid }}
    "fushaarid": {{ .Params.fushaarid | jsonify }},
    {{end}}
    {{ if .Params.fushaarserver }}
    "fushaarserver":{
        {{ if  .Params.fushaarserver.q240 }}
            "q240": {{ .Params.fushaarserver.q240 |jsonify }},
        {{end}}
        {{ if  .Params.fushaarserver.q480 }}
            "q480": {{ .Params.fushaarserver.q480 |jsonify }},
        {{end}}
        {{ if  .Params.fushaarserver.q1080 }}
            "q1080": {{ .Params.fushaarserver.q1080 |jsonify }}
        {{end}}
    },
    {{end}}

    {{ if .Params.server2 }}
    "server2": {{ .Params.server2 | jsonify }},
    {{ end }}

    {{ if .Params.server3 }}
    "server3": {{ .Params.server3 | jsonify }},
    {{ end }}

    {{ if .Params.server4 }}
    "server4": {{ .Params.server4 | jsonify }},
    {{ end }}

    {{ if .Params.server5 }}
    "server5": {{ .Params.server5 | jsonify }},
    {{ end }}

    {{ if .Params.server6 }}
    "server6": {{ .Params.server6 | jsonify }},
    {{ end }}

    {{- if .Params.story -}}
    "story": {{- .Params.story | jsonify -}},
    {{- end -}}

{{ $related := .Site.RegularPages.Related . | first 5 }}

{{ with $related }}
    "related":[
        {{ range $index, $e := .}}
        {{- if $index -}},{{- end -}}
        {
            "title": {{ .Params.title | jsonify }},
            "rating": {{ .Params.moviecover | jsonify }},
            "rating": {{ .Params.rating | jsonify }},
            "link": {{ .Params.slug | jsonify}}
        }
        {{end}}
    ]    
{{end}}
    
}   
